{
  "name": "hnto/nuki",
  "type": "framework",
  "description": "a new and flexible Nano PHP Framework",
  "authors": [
    {
      "name": "Herant Awadisan",
      "email": "herant.awadisan@gmail.com"
    }
  ],
  "keywords": [
    "framework", "library", "nuki"
  ],
  "require": {
    "php": ">=7.0.0",
    "monolog/monolog": "1.22.1",
    "ralouphie/getallheaders": "2.0.5",
    "zendframework/zend-stdlib": "3.1.0",
    "zendframework/zend-hydrator": "2.2.1",
    "zendframework/zend-filter": "2.7.1",
    "zendframework/zend-servicemanager": "3.3.0",
    "pimple/pimple": "3.0.2",
    "foil/foil": "0.6.6",
    "vlucas/phpdotenv": "^2.4",
    "adbario/php-dot-notation": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Nuki\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "6.0"
  }
}
